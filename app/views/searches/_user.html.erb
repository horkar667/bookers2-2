<table class="table">
   <thead>
     <tr>
       <th></th>
       <th>name</th>
       <th>introduction</th>
       <th></th>
     </tr>
   </thead>
  <% users.each do |user| %>
   <tbody>
     <tr>
       <td>
        <%= attachment_image_tag user, :profile_image, :fill, 40, 40, format: 'jpeg' %>
       </td>
       <td><%= user.name %></td>
       <td><%= user.introduction %></td>
       <td><%= link_to "Show", user_path(user) %></td>
     </tr>
   </tbody>
  <% end %>
 </table>